  "scripts": {
    "docker:db": "docker-compose -f docker-compose.yml up -d redis",
    "docker:build": "docker build -t config-microservice-example .",
    "docker:run": "docker run -p 3000:3000 config-microservice-example",
    "docker:stop": "docker stop $(docker ps -a -q --filter ancestor=config-microservice-example --format=\"{{.ID}}\")",
    "docker:down": "docker-compose -f docker-compose.yml down",
    "docker:restart": "docker-compose -f docker-compose.yml restart",

    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "ioredis": "^5.3.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/microservices": "^10.3.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
